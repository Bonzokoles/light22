{
  "name": "@parcel/css",
  "version": "1.0.0-alpha.5",
  "license": "MIT",
  "description": "A CSS parser, transformer, and minifier written in Rust",
  "main": "native.js",
  "types": "node/index.d.ts",
  "targets": {
    "main": false
  },
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/parcel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/parcel-bundler/parcel-css.git"
  },
  "engines": {
    "node": ">= 12.0.0",
    "parcel": "^2.0.0-beta.1"
  },
  "napi": {
    "name": "parcel-css"
  },
  "files": [
    "native.js",
    "*.node"
  ],
  "dependencies": {
    "detect-libc": "^1.0.3"
  },
  "devDependencies": {
    "@mdn/browser-compat-data": "^4.0.12",
    "@napi-rs/cli": "1.0.4",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.0",
    "bootstrap": "^4.6.1",
    "caniuse-lite": "^1.0.30001272",
    "cssnano": "^5.0.8",
    "csso": "^5.0.0",
    "esbuild": "^0.13.10",
    "parcel": "^2.0.1",
    "postcss": "^8.3.11",
    "tailwindcss": "^2.2.19"
  },
  "scripts": {
    "build": "node build.js",
    "build-release": "node build.js --release",
    "wasm:build": "wasm-pack build node --target nodejs",
    "wasm:build-release": "wasm-pack build node --target nodejs --release",
    "wasm-browser:build": "wasm-pack build node --target web",
    "wasm-browser:build-release": "wasm-pack build node --target web --release",
    "playground:start": "parcel playground/index.html",
    "playground:build": "yarn wasm-browser:build-release && parcel build playground/index.html"
  }
}
